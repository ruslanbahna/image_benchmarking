podman inspect mcr.microsoft.com/openjdk/jdk:21-ubuntu | jq -r '.[0].Config.Labels."org.opencontainers.image.ref.name" + ":" + .[0].Config.Labels."org.opencontainers.image.version" + ":" + (.[0].Config.Env[] | select(startswith("JAVA_HOME=")) | capture("msopenjdk-(?<version>\\d+)").version)'
podman inspect mcr.microsoft.com/openjdk/jdk:21-ubuntu | jq -r '.[0].Config.Labels."org.opencontainers.image.ref.name" + ":" + .[0].Config.Labels."org.opencontainers.image.version" + ":" + (.[0].Config.Env[] | select(startswith("JAVA_HOME=")) | capture("msopenjdk-(?<version>\\d+)") // empty | .version // "unknown")'
